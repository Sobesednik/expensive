  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App",
      "program": "${workspaceFolder}/src/bin/register.js",
      "env": {
        "BABEL_ENV": "debug",
        "NODE_DEBUG": "expensive"
      },
      "args": [
        // "zavr.co.uk",
        "test",
        // "testt",
        // "testtt",
        // "-z",
        // "com,cc,co,io",
        // "-f"
        // "-i"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "args": [
        // "test/spec/query.js",
        "test/spec/namecheap/domains/get-info.js",
        "-b",
        "-w"
      ],
      "env": {
        "BABEL_ENV": "debug",
        "NODE_DEBUG": "expensive",
        "ZOROASTER_TIMEOUT": "9999999"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
