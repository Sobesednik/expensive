{
  "name": "expensive",
  "version": "2.0.1",
  "description": "A Namecheap.com Client To Register And Manage Domains From The CLI.",
  "scripts": {
    "t": "SANDBOX=1 zoroaster -a -t 100000",
    "mask": "yarn t test/mask",
    "spec": "yarn t test/spec",
    "test": "yarn t test/spec test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "compile": "depack-dev src/bin/expensive -o compile/bin/expensive.js -a -c -p -s -O 2018 --externs types/externs.js --source_map_include_content",
    "dev": "SANDBOX=1 NODE_DEBUG=expensive node src/bin",
    "start": "node src/bin",
    "lint": "eslint .",
    "e": "alanode",
    "man": "man man/doc.1",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "wiki": "NODE_DEBUG=doc doc wiki -W ../expensive.wiki",
    "example/": "yarn e example/simple.js",
    "externs": "typal types/externs.js",
    "d": "typal types/index.js -c",
    "build": "yarn-s b doc",
    "args": "argufy types/args/* -o src/bin/get-args.js",
    "splendid": "splendid",
    "web": "splendid -s"
  },
  "files": [
    "compile"
  ],
  "bin": {
    "expensive": "compile/bin/expensive.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/expensive.git"
  },
  "keywords": [
    "namecheap",
    "domains",
    "domain",
    "register",
    "DNS",
    "devops",
    "dev ops",
    "api",
    "expensive",
    "whois"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/expensive/issues"
  },
  "homepage": "https://expensive.page",
  "devDependencies": {
    "@rqt/aqt": "^1.3.0",
    "@rqt/ip": "^1.2.2",
    "@rqt/namecheap": "^2.4.2",
    "@rqt/namecheap-web": "^1.4.2",
    "africa": "^1.3.1",
    "alamode": "^2.3.6",
    "argufy": "^1.7.1",
    "bosom": "^2.0.0",
    "clearr": "^1.0.0",
    "documentary": "^1.29.0",
    "erotic": "^2.1.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "^1.0.1",
    "forkfeed": "^1.1.1",
    "frame-of-mind": "^1.2.3",
    "indicatrix": "^1.3.0",
    "preact": "8.4.2",
    "promto": "^1.1.2",
    "reloquent": "^1.3.2",
    "rqt": "^4.0.0",
    "spawncommand": "^2.2.0",
    "tablature": "^1.2.0",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {}
}
