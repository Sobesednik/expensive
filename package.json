{
  "name": "expensive",
  "version": "1.6.0",
  "description": "A Namecheap.com Client To Register And Manage Domains From The CLI.",
  "scripts": {
    "t": "SANDBOX=1 zoroaster -a -t 100000",
    "mask": "yarn t test/mask",
    "spec": "yarn t test/spec",
    "test": "yarn t test/spec test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "dev": "SANDBOX=1 NODE_DEBUG=expensive node src/bin",
    "start": "node src/bin",
    "lint": "eslint .",
    "e": "node example",
    "man": "man man/doc.1",
    "doc": "NODE_DEBUG=documentary doc documentary -o README.md",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "example/": "yarn e example/simple.js",
    "b": "alamode src -o build -s -i bin/index.js,bin/.eslintrc,lib/print/.eslintrc",
    "build": "yarn-s b doc"
  },
  "files": [
    "build"
  ],
  "bin": {
    "expensive": "build/bin/expensive.js"
  },
  "man": "./man/doc.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/expensive.git"
  },
  "keywords": [
    "namecheap",
    "domains",
    "domain",
    "register",
    "DNS",
    "devops",
    "dev ops",
    "api",
    "expensive",
    "whois"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/expensive/issues"
  },
  "homepage": "https://expensive.page",
  "devDependencies": {
    "alamode": "1.6.0",
    "documentary": "1.20.1",
    "eslint-config-artdeco": "1.0.1",
    "forkfeed": "1.0.0",
    "yarn-s": "1.1.0",
    "zoroaster": "3.6.6"
  },
  "dependencies": {
    "@rqt/ip": "1.1.0",
    "@rqt/namecheap": "2.1.1",
    "@rqt/namecheap-web": "1.3.1",
    "africa": "1.2.0",
    "argufy": "1.2.1",
    "bosom": "1.0.0",
    "erotic": "2.0.1",
    "erte": "1.1.4",
    "frame-of-mind": "1.0.0",
    "indicatrix": "1.0.0",
    "promto": "1.0.1",
    "reloquent": "1.2.3",
    "rqt": "3.0.1",
    "tablature": "1.0.0",
    "usually": "1.0.0"
  }
}
