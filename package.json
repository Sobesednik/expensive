{
  "name": "expensive",
  "version": "1.4.1",
  "description": "Namecheap.com API client to register and manage domains from CLI.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn t test/spec",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "start": "node build/bin",
    "dev": "SANDBOX=1 NODE_DEBUG=expensive node src/bin/register",
    "s": "node build/bin",
    "lint": "eslint .",
    "e": "node example",
    "man": "man man/doc.1",
    "m": "man man/doc.1",
    "doc": "doc documentary -o README.md",
    "dc": "git add README-source.md README.md && git commit -m ",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "example/": "yarn e example/simple.js",
    "b": "b --source-maps --copy-files --ignore src/bin/register.js",
    "build": "yarn-s build-src doc"
  },
  "files": [
    "build"
  ],
  "bin": {
    "expensive": "build/bin/index.js"
  },
  "man": "./man/doc.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/expensive.git"
  },
  "keywords": [
    "namecheap",
    "domains",
    "api",
    "expensive"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/expensive/issues"
  },
  "homepage": "https://github.com/artdecocode/expensive#readme",
  "devDependencies": {
    "documentary": "1.9.0",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "africa": "1.2.0",
    "argufy": "1.2.1",
    "bosom": "1.0.0",
    "catchment": "2.0.1",
    "erotic": "1.2.1",
    "erte": "1.1.4",
    "promto": "1.0.1",
    "reloquent": "1.0.6",
    "rqt": "2.0.0",
    "tablature": "1.0.0",
    "usually": "1.0.0"
  }
}
